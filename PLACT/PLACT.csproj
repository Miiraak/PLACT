    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <Version>1.0.1</Version>
    <ApplicationIcon>logo.ico</ApplicationIcon>
    <Authors>Miiraak</Authors>
    <PackageProjectUrl></PackageProjectUrl>
